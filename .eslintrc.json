{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "brace-style": [
      "error",
      "allman"
    ],
    "no-underscore-dangle": 0,
    "import/extensions": 0
  }
}
